{
  "name": "React Facet Dev Tools for Chrome",
  "description": "TODO",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "devtools_page": "main.html",
  "web_accessible_resources": [
    "main.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
